{
  "presets": [
    "react",
    ["es2015", {
      "modules": false
    }],
    "stage-1",
    "stage-2",
    "stage-3"
  ],
  "plugins": [
    ["import", {
      "libraryName": "antd",
      "style": "css"
    }],
    "react-hot-loader/babel",
    "transform-runtime",
    "transform-es2015-modules-commonjs",
    "transform-react-jsx"
  ]
}
