{
  "presets": [
    "react",
    "es2015",
    "stage-1",
    "stage-2",
    "stage-3",
    [
      "env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    ["import", {
      "libraryName": "antd",
      "style": "css"
    }],
    "transform-class-properties",
    "syntax-dynamic-import"
  ]
}